from abc import ABC, abstractmethod

# Define the Metal class here
class Metal(ABC):
    @abstractmethod
    def get_melting_point(self):
        pass 
    
# Define the Child classes here
class Aluminum(Metal):
    def get_melting_point(self):
        return 660

class Copper(Metal):
    def get_melting_point(self):
        return 1084
    
class Gold(Metal):
    def get_melting_point(self):
        return 1063


g = Gold()
g.get_melting_point()



