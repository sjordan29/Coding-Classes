class Calculator:
    def __init__(self, num1, num2):
        self.num1 = num1
        self.num2 = num2

    def add(self):
        return self.num1 + self.num2

    def subtract(self):
        return self.num2 - self.num1

    def multiply(self):
        return self.num1 * self.num2

    def divide(self):
        return self.num2 / self.num1



obj = Calculator(10, 94);
print(obj.add())
print(obj.subtract())
print(obj.multiply())
print(obj.divide())


class Rectangle:
    def __init__(self, length, width):
        self.__length = length
        self.__width = width

    def area(self):
        return self.__length * self.__width

    def perimeter(self):
        return 2 * (self.__length + self.__width)


r = Rectangle(4,5)
print(f"Area: {r.area()}")
print(f"Perimeter: {r.perimeter()}")


